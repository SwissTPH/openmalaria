            units:Dimensionless;min:0.0;max:1.0;name:Effectiveness of treatment in non-adherent patients;</xs:appinfo>
    </xs:annotation>
   </xs:element>
   <xs:element name="treatmentActions">
    <xs:complexType>
     <xs:all>
      <xs:element name="CQ" type="om:TreatmentOption" minOccurs="0"/>
